    <PackageReference Include="Radzen.Blazor" Version="4.13.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Client\2Parcial_BonillaAp1.Client.csproj" />
    <ProjectReference Include="..\Shared\2Parcial_BonillaAp1.Shared.csproj" />
  </ItemGroup>


</Project>
